name: dockercompose16821409691285368182
services:
  database:
    container_name: database_container
    environment:
      ACCEPT_EULA: "y"
      MSSQL_SA_PASSWORD: Secret123!
    image: mcr.microsoft.com/mssql/server
    networks:
      default: null
    ports:
    - mode: ingress
      target: 1433
      published: "1433"
      protocol: tcp
    volumes:
    - type: bind
      source: C:\CODING\Interviews\Danske\tax-sql-volume
      target: /var/opt/mssql/data
      bind:
        create_host_path: true
  taxesmanager.api:
    build:
      context: C:\CODING\Interviews\Danske\TaxesManager
      dockerfile: TaxesManager.API/Dockerfile
      labels:
        com.microsoft.created-by: visual-studio
        com.microsoft.visual-studio.project-name: TaxesManager.API
      target: base
    container_name: TaxesManager.API
    depends_on:
      database:
        condition: service_started
    entrypoint:
    - tail
    - -f
    - /dev/null
    environment:
      ASPNETCORE_ENVIRONMENT: Development
      ASPNETCORE_LOGGING__CONSOLE__DISABLECOLORS: "true"
      ASPNETCORE_URLS: https://+:443;http://+:80
      DOTNET_USE_POLLING_FILE_WATCHER: "1"
      NUGET_FALLBACK_PACKAGES: ""
    image: taxesmanagerapi:dev
    labels:
      com.microsoft.visualstudio.debuggee.arguments: ' --additionalProbingPath /root/.nuget/packages  "/app/bin/Debug/net6.0/TaxesManager.API.dll"'
      com.microsoft.visualstudio.debuggee.killprogram: /bin/sh -c "if PID=$$(pidof
        dotnet); then kill $$PID; fi"
      com.microsoft.visualstudio.debuggee.program: dotnet
      com.microsoft.visualstudio.debuggee.workingdirectory: /app
    networks:
      default: null
    ports:
    - mode: ingress
      target: 80
      protocol: tcp
    - mode: ingress
      target: 443
      protocol: tcp
    tty: true
    volumes:
    - type: bind
      source: C:\CODING\Interviews\Danske\TaxesManager\TaxesManager.API
      target: /app
      bind:
        create_host_path: true
    - type: bind
      source: C:\Users\Rapolas\vsdbg\vs2017u5
      target: /remote_debugger
      bind:
        create_host_path: true
    - type: bind
      source: C:\Users\Rapolas\AppData\Roaming/ASP.NET/Https
      target: /root/.aspnet/https
      read_only: true
      bind:
        create_host_path: true
    - type: bind
      source: C:\Users\Rapolas\AppData\Roaming/Microsoft/UserSecrets
      target: /root/.microsoft/usersecrets
      read_only: true
      bind:
        create_host_path: true
    - type: bind
      source: C:\Users\Rapolas\.nuget\packages\
      target: /root/.nuget/packages
      read_only: true
      bind:
        create_host_path: true
    - type: bind
      source: C:\CODING\Interviews\Danske\TaxesManager
      target: /src
      bind:
        create_host_path: true
networks:
  default:
    name: dockercompose16821409691285368182_default